# AI Tool Secrets
# Copy this template into your encrypted secrets file:
#   ai-secrets-edit
# Then fill in your actual values.

# NetBox
NETBOX_URL=https://netbox.example.com
NETBOX_TOKEN=

# GitHub
GITHUB_PERSONAL_ACCESS_TOKEN=

# Grafana
GRAFANA_URL=https://grafana.example.com
GRAFANA_API_KEY=

# InfluxDB 1.x
INFLUXDB_URL=http://influxdb.example.com:8086
INFLUXDB_USER=
INFLUXDB_PASSWORD=

# SolarWinds Orion (queried via curl + SWQL, no MCP server)
SOLARWINDS_URL=https://solarwinds.example.com:17778
SOLARWINDS_USER=
SOLARWINDS_PASSWORD=

# Juniper MCP server paths (default: ~/ai/mcp/juniper)
JUNIPER_MCP_DIR=~/ai/mcp/juniper
JUNIPER_MCP_DEVICES=~/ai/mcp/juniper/devices.json

# InfluxDB v1 MCP server path (default: ~/ai/mcp/influxdb-v1)
INFLUXDB_V1_MCP_DIR=~/ai/mcp/influxdb-v1

# Ruckus SmartZone (queried via curl + REST API, no MCP server)
RUCKUS_SZ_URL=https://smartzone.example.com:8443
RUCKUS_SZ_USERNAME=
RUCKUS_SZ_PASSWORD=
